using namespace std;

int main()

{

    int a, b, c, d;

    cin>>a>>b>>c>>d;

    int t1, t2, t3, t4;

    t1=0;

    t2=a;

    t3=a+b;

    t4=a+b+c;

    printf("P1 (WT=%d), P2 (WT=%d), P3 (WT=%d), P4 (WT=%d)", t1, t2, t3, t4);

    

